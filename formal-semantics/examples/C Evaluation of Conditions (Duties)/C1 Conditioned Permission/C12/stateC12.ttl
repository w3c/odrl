@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix sotw: <https://w3id.org/force/sotw#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix pay: <https://www.epimorphics.com/guide-to-the-payments-ontology/> .
@prefix ex:   <https://ex.org/> .

ex:C12 a sotw:SotW ;
    sotw:context ex:C12Context-A, ex:C12Context-B .

ex:C12Context-A a pay:Payment ;
    sotw:conditionID <http://example.com/condition/1> ;
    pay:payer <http://example.com/party/billie> ;
    pay:payee <http://example.com/party/sony> ;
    pay:reference "UGQEHUR6" ;
    pay:netAmount "5.00"^^xsd:decimal ;
    pay:currency <https://dbpedia.org/resource/Euro> ;
    pay:paymentDate "2025-07-23 00:00:00" .
